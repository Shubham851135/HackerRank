class Solution(object):
    def addBinary(self, a, b):
        i,j=len(a)-1,len(b)-1
        Carry=0
        result=""

        while i>=0 or j>=0 or Carry:
            Bit1=int(a[i]) if i>=0 else 0
            Bit2=int(b[j]) if j>=0 else 0

            Tottal=Bit1+Bit2+Carry
            result = str(Tottal % 2) + result
            Carry = Tottal//2


            i=i-1
            j=j-1
        return result

